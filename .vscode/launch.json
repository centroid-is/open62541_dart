    "version": "0.2.0",
    "configurations": [
        {
            "name": "async_integration_test (debug - linux)",
            "type": "cppdbg",
            "request": "launch",
            // path to the Dart executable (in the downloaded/installed SDK).
            "program": "/home/dl/flutter/bin/cache/dart-sdk/bin/dart",
            "args": [
                // Path to the standalone Dart file that invokes your shared library.
                "${workspaceFolder}/test/async_integration_test.dart",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb", // Provided that your host OS is Linux.
            // "sourceFileMap": {
            //     // When building the SDK locally, this gives source locations of  the stuff in the Dart
            //     // executable. A downloaded SDK probably does not have symbols. But if you're
            //     // only trying to debug your shared library that should be fine.
            //     "../../out/DebugX64/../../": "${workspaceFolder}/"
            // },
        },
        {
            "name": "server_example (debug - macos)",
            "type": "cppdbg",
            "request": "launch",
            "program": "/Users/omar/source/repos/flutter/bin/cache/dart-sdk/bin/dart",
            "args": [
                "${workspaceFolder}/example/server_example.dart"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            //"preLaunchTask": "build",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "sourceFileMap": {
                "/Users/omar/source/repos/open62541_bindings": "${workspaceFolder}"
            }
        },
    ]
}